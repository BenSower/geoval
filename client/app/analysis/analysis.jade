div(ng-include='"components/navbar/navbar.html"')
.container
    table.table
        thead
          tr
            th Trajectory Name
            th Length
            th Outlier Threshold
        tbody(ng-repeat='trajectory in rawTrajectories')
          tr
            td {{trajectory.id}}
            td {{trajectory.geometry.coordinates.length}}
            td {{trajectory.properties.outlierThreshold.toFixed(2)}}m
          
